<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Unknown0</th>
      <th>Unknown1</th>
      <th>Shooting range</th>
      <th>Marksmen</th>
      <th>Unknown2</th>
      <th>Unknown3</th>
      <th>Shoot number</th>
      <th>Value</th>
      <th>Unknown4</th>
      <th>Unknown5</th>
      <th>Unknown6</th>
      <th>Unknown7</th>
      <th>Unknown8</th>
      <th>Time</th>
      <th>Unknown9</th>
      <th>Date</th>
      <th>Unknown10</th>
      <th>Unknown11</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.unknown0 %></td>
        <td><%= event.unknown1 %></td>
        <td><%= event.shooting_range %></td>
        <td><%= event.marksmen %></td>
        <td><%= event.unknown2 %></td>
        <td><%= event.unknown3 %></td>
        <td><%= event.shoot_number %></td>
        <td><%= event.value %></td>
        <td><%= event.unknown4 %></td>
        <td><%= event.unknown5 %></td>
        <td><%= event.unknown6 %></td>
        <td><%= event.unknown7 %></td>
        <td><%= event.unknown8 %></td>
        <td><%= event.time %></td>
        <td><%= event.unknown9 %></td>
        <td><%= event.date %></td>
        <td><%= event.unknown10 %></td>
        <td><%= event.unknown11 %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
