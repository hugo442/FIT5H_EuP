<%= form_tag("/analysis/calculate", method: "get") do %>
  
	<% cities_array = @events.map { |city| [city.name, city.name] } %>
	<%= select_tag "eventname", options_for_select(cities_array) %>

	<% method_array = @method.map { |city| [city, city] } %>
	<%= select_tag "Verfahren", options_for_select(method_array) %>
  
  <%= submit_tag("Do it") %>
<% end %>